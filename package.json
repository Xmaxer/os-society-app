{
	"name": "os_society_app",
	"version": "1.0.0",
	"private": true,
	"dependencies": {
		"@testing-library/jest-dom": "4.2.4",
		"@testing-library/react": "9.5.0",
		"@testing-library/user-event": "7.2.1",
		"react": "16.13.1",
		"react-dom": "16.13.1",
		"react-scripts": "3.4.1",
		"@material-ui/core": "4.11.0",
		"@material-ui/icons": "4.9.1",
		"@material-ui/lab": "4.0.0-alpha.56",
		"@material-ui/pickers": "4.0.0-alpha.12",
		"react-router-dom": "5.2.0",
		"graphql-hooks": "4.5.0",
		"universal-cookie": "4.0.3",
		"formik": "2.1.4",
		"date-fns": "2.14.0",
		"clsx": "1.1.1",
		"@date-io/date-fns": "1.3.13",
		"typescript": "3.9.7",
		"@types/node": "14.10.1",
		"@types/react": "16.9.49",
		"@types/react-dom": "16.9.8",
		"@types/react-router-dom": "5.1.5",
		"@types/jest": "26.0.13",
		"graphql": "15.3.0",
		"graphql-tag": "2.11.0"
	},
	"devDependencies": {
		"apollo": "2.30.3",
		"prettier": "2.1.1"
	},
	"scripts": {
		"install-modules": "pnpm install",
		"start": "react-scripts start",
		"build": "GENERATE_SOURCEMAP=false react-scripts build",
		"test": "react-scripts test --passWithNoTests",
		"eject": "react-scripts eject",
		"prettier": "prettier --write .",
		"prettier-ci": "prettier --check .",
		"graphql-generate": "npm run graphql-generate-schema && npm run graphql-generate-typescript-interfaces",
		"graphql-generate-typescript-interfaces": "apollo client:codegen --includes=./src/assets/api/queries.ts --localSchemaFile=./src/assets/api/schema.json --target=typescript --outputFlat ./src/assets/api/apiInterfaces.ts",
		"graphql-generate-schema": "apollo client:download-schema --endpoint=http://192.168.79.131:3000/api ./src/assets/api/schema.json"
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	}
}
